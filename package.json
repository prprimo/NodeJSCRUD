    "type": "git",
    "url": "https://github.com/prprimo/NodeJSCRUD.git"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.42.0",
    "tedious": "^4.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.18",
    "nodemon": "^1.18.9",
    "sequelize-typescript": "^1.0.0-alpha.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2"
  }
