{
  "name": "nodejscrud",
  "version": "1.0.0",
  "description": "NODEJS CRUD (Back-End)",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./src/server.ts",
    "start": "node --require ts-node/register index.ts",
    "start1": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/server.ts",
    "start2": "nodemon ./dist/server.js",
    "prod": "npm run build && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
